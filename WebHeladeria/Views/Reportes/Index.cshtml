@model IEnumerable<Reporte>

<!DOCTYPE html>
<html>
<head>
    <title>Reporte de Gustos</title>
</head>
<body>
    <h1>Reporte de Gustos Más Pedidos</h1>
    <table>
        <thead>
            <tr>
                <th>ID del Gusto</th>
                <th>Nombre del Gusto</th>
                <th>Cantidad Total Pedida</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var reporte in Model)
            {
                <tr>
                    <td>@reporte.IdGusto</td>
                    <td>@reporte.NombreGusto</td>
                    <td>@reporte.CantidadTotalPedida</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>

